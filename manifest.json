{
  "manifest_version": 2,
  "name": "Yara's New Tab",
  "version": "1.1.3",
  "short_name": "YNT",
  "description": "A metro style inspired new tab with customizable tiles and windows like experience.",
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "author": "Mehdi Qanavatian",
  "icons": {
    "16": "/RES/UI/icon_16.png",
    "24": "/RES/UI/icon_24.png",
    "48": "/RES/UI/icon_48.png",
    "64": "/RES/UI/icon_64.png",
    "96": "/RES/UI/icon_96.png",
    "128": "/RES/UI/icon_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "/RES/UI/icon_16.png"
  }
}